   "source": [
    "# Factorial- Iterative"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lwf(n):\n",
    "    j = 1\n",
    "    for i in range(2,n+1):\n",
    "        j = j*i\n",
    "    return j"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
